{
	"name": "logigator-backend",
	"version": "1.1.2",
	"description": "Logigator is a simple, yet powerful web-based online tool to build and simulate logic circuits.",
	"contributors": [
		{
			"name": "Andreas Schlößl",
			"email": "andreas.sch4@gmail.com"
		},
		{
			"name": "Leo Preissegger",
			"email": "preissegger.leo@gmail.com"
		},
		{
			"name": "Alex Mittermüller"
		},
		{
			"name": "Stefan Mikulik"
		}
	],
	"keywords": [
		"logigator",
		"circuit",
		"logic",
		"simulator",
		"web",
		"online"
	],
	"homepage": "https://logigator.com/",
	"bugs": "https://github.com/logigator/logigator/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/logigator/logigator.git"
	},
	"scripts": {
		"start": "concurrently \"tsc -p tsconfig.json -w\" \"nodemon -i resources/public/ -i resources/private/components -i resources/private/projects dist/main.js\"",
		"prebuild": "rimraf dist",
		"build": "tsc -p tsconfig.json",
		"start:prod": "node dist/main.js",
		"lint:backend": "eslint -c ./src/.eslintrc.js \"src/**/*.ts\"",
		"lint:frontend": "eslint -c ./resources/private/js/.eslintrc.js \"resources/private/js/**/*.js\"",
		"migration:build": "rimraf migration/*.js && tsc migration/*.ts",
		"migration:show": "yarn run migration:build && typeorm migration:show -f config/ormconfig.json",
		"migration:run": "yarn run migration:build && typeorm migration:run -f config/ormconfig.json",
		"migration:revert": "yarn run migration:build && typeorm migration:revert -f config/ormconfig.json",
		"migration:generate": "yarn run migration:build && typeorm migration:generate -f config/ormconfig.json"
	},
	"main": "./dist/main.js",
	"dependencies": {
		"@types/uuid": "^8.3.3",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"compression": "^1.7.4",
		"connect-redis": "^6.1.3",
		"cookie-parser": "^1.4.6",
		"express": "^4.17.1",
		"express-handlebars": "^6.0.2",
		"express-session": "^1.17.2",
		"md5": "^2.3.0",
		"mime": "^3.0.0",
		"multer": "^1.4.3",
		"mysql": "^2.18.1",
		"mysql2": "^2.3.3",
		"node-fetch": "^2.6.6",
		"nodemailer": "^6.7.2",
		"passport": "^0.6.0",
		"passport-google-oauth": "^2.0.0",
		"passport-local": "^1.0.0",
		"passport-twitter": "^1.0.4",
		"redis": "^4.5.1",
		"reflect-metadata": "^0.1.13",
		"routing-controllers": "^0.9.0",
		"typedi": "^0.8.0",
		"typeorm": "^0.2.41",
		"typeorm-typedi-extensions": "^0.4.1",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@types/bcrypt": "^3.0.0",
		"@types/body-parser": "^1.19.2",
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.13",
		"@types/express-handlebars": "^6.0.0",
		"@types/express-session": "^1.17.4",
		"@types/md5": "^2.3.1",
		"@types/mime": "^2.0.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.11.10",
		"@types/node-fetch": "^3.0.3",
		"@types/nodemailer": "^6.4.4",
		"@types/passport": "^1.0.11",
		"@types/passport-google-oauth": "^1.0.42",
		"@types/passport-local": "^1.0.34",
		"@types/passport-twitter": "^1.0.37",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"concurrently": "^6.4.0",
		"core-js-bundle": "^3.19.1",
		"cropperjs": "^1.5.12",
		"element-closest-polyfill": "^1.0.4",
		"eslint": "^7.32.0",
		"gulp": "^4.0.2",
		"gulp-add-src": "^1.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-clean-css": "^4.3.0",
		"gulp-concat": "^2.6.1",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.0.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-terser": "^2.1.0",
		"nodemon": "^2.0.15",
		"regenerator-runtime": "^0.13.9",
		"rimraf": "^3.0.2",
