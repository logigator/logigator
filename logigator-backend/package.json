		"@types/body-parser": "^1.19.5",
		"@types/compression": "^1.7.5",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/express-handlebars": "^6.0.0",
		"@types/express-session": "^1.18.0",
		"@types/md5": "^2.3.5",
		"@types/mime": "^3.0.4",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.11.25",
