{
  "name": "logigator-editor",
  "version": "1.1.2",
  "description": "Logigator Editor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logigator/logigator.git"
  },
  "scripts": {
    "start": "yarn run start:web",
    "start:web": "yarn run build-custom-build-scripts && yarn run cp-sim-wasm-assets && env-cmd -e dev-web --use-shell \"ng serve -c dev-web\"",
    "build": "yarn run build:web",
    "build:web": "yarn run build-custom-build-scripts && yarn run cp-sim-wasm-assets && env-cmd -e prod-web --use-shell \"ng build -c prod-web \" && yarn run wasm-js-cache-bust",
    "cp-sim-wasm-assets": "node custom-build-scripts/dist/copy-simulation-wasm-assets.js",
    "wasm-js-cache-bust": "node custom-build-scripts/dist/wasm-js-cache-bust.js",
    "build-custom-build-scripts": "tsc -p custom-build-scripts/tsconfig-custom-build-scripts.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@logigator/logigator-simulation": "^1.2.0",
    "@ngx-translate/core": "^15.0.0",
    "bootstrap": "^4.6.2",
    "form-data": "^4.0.0",
    "github-markdown-css": "^4.0.0",
    "ngx-toastr": "^18.0.0",
    "node-fetch": "^3.3.2",
    "pixi.js": "^5.3.9",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/language-service": "^17.0.8",
    "@ngtools/webpack": "^17.0.8",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.10",
    "codelyzer": "^6.0.2",
    "env-cmd": "^10.1.0",
    "jasmine-core": "~3.99.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.5.4",
    "markdown-loader": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "tslint": "^6.1.3",
    "typescript": "5.2.2",
    "wait-on": "^6.0.1",
    "webpack": "^5.59.1",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "developmentInfo": {
    "": "For Development you should use the local version of shared-cops to use live reload from the libary",
    "@logigator/logigator-simulation": "file:../../logigator-simulation"
  }
}
