{
  "name": "logigator-editor",
  "version": "1.1.2",
  "description": "Logigator Editor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logigator/logigator.git"
  },
  "scripts": {
    "start": "yarn run start:web",
    "start:web": "yarn run build-custom-build-scripts && yarn run cp-sim-wasm-assets && env-cmd -e dev-web --use-shell \"ng serve -c dev-web\"",
    "build": "yarn run build:web",
    "build:web": "yarn run build-custom-build-scripts && yarn run cp-sim-wasm-assets && env-cmd -e prod-web --use-shell \"ng build -c prod-web \" && yarn run wasm-js-cache-bust",
    "cp-sim-wasm-assets": "node custom-build-scripts/dist/copy-simulation-wasm-assets.js",
    "wasm-js-cache-bust": "node custom-build-scripts/dist/wasm-js-cache-bust.js",
    "build-custom-build-scripts": "tsc -p custom-build-scripts/tsconfig-custom-build-scripts.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.4.0",
    "@angular/common": "^13.4.0",
    "@angular/compiler": "^13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/forms": "^13.4.0",
    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "@angular/router": "^13.4.0",
    "@logigator/logigator-simulation": "^1.2.0",
    "@ngx-translate/core": "^13.0.0",
    "bootstrap": "^4.6.1",
    "form-data": "^4.0.0",
    "github-markdown-css": "^4.0.0",
    "ngx-toastr": "^13.2.1",
    "node-fetch": "^2.6.6",
    "pixi.js": "^5.3.9",
    "run-script-os": "^1.1.6",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.4.0",
    "@angular/language-service": "^13.4.0",
    "@ngtools/webpack": "^13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^16.11.10",
    "@types/node-fetch": "^2.5.12",
    "codelyzer": "^6.0.2",
    "env-cmd": "^10.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "markdown-loader": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "4.6.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.30.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "developmentInfo": {
    "": "For Development you should use the local version of shared-cops to use live reload from the libary",
    "@logigator/logigator-simulation": "file:../../logigator-simulation"
  }
}
