{
	"name": "logigator-editor",
	"version": "0.0.0",
	"description": "Logigator Editor",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/logigator/logigator.git"
	},
	"scripts": {
		"start": "yarn run start:web",
		"start:web": "yarn run build-custom-build-scripts && yarn run cp-sim-wasm-assets && ng serve",
		"build": "yarn run build:web",
		"build:web": "yarn run build-custom-build-scripts && yarn run cp-sim-wasm-assets && ng build && yarn run wasm-js-cache-bust",
		"cp-sim-wasm-assets": "node custom-build-scripts/dist/copy-simulation-assets.js",
		"wasm-js-cache-bust": "node custom-build-scripts/dist/wasm-js-cache-bust.js",
		"build-custom-build-scripts": "tsc -p custom-build-scripts/tsconfig-custom-build-scripts.json",
		"lint": "ng lint && tsc-strict",
		"lint:fix": "ng lint --fix",
		"format": "prettier . --check",
		"format:fix": "prettier . --write"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^17.0.8",
		"@angular/common": "^17.0.8",
		"@angular/compiler": "^17.0.8",
		"@angular/core": "^17.0.8",
		"@angular/forms": "^17.0.8",
		"@angular/platform-browser": "^17.0.8",
		"@angular/platform-browser-dynamic": "^17.0.8",
		"@angular/router": "^17.0.8",
		"@logigator/logigator-simulation": "^1.2.0",
		"@ngx-translate/core": "^15.0.0",
		"bootstrap": "^4.6.2",
		"github-markdown-css": "^4.0.0",
		"marked": "^9.0.3",
		"ngx-markdown": "^17.1.1",
		"ngx-toastr": "^18.0.0",
		"pixi.js": "^5.3.9",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.2",
		"uuid": "^9.0.1",
		"vanilla-cookieconsent": "^3.0.0",
		"zone.js": "~0.14.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.0.8",
		"@angular-eslint/builder": "17.1.1",
		"@angular-eslint/eslint-plugin": "17.1.1",
		"@angular-eslint/eslint-plugin-template": "17.1.1",
		"@angular-eslint/schematics": "17.1.1",
		"@angular-eslint/template-parser": "17.1.1",
		"@angular/cli": "^17.0.8",
		"@angular/compiler-cli": "^17.0.8",
		"@angular/language-service": "^17.0.8",
		"@types/jasmine": "^5.1.4",
		"@types/node": "^20.10.5",
		"@typescript-eslint/eslint-plugin": "6.13.1",
		"@typescript-eslint/parser": "6.13.1",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.1.0",
		"jasmine-core": "^5.1.1",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-jasmine": "^5.1.0",
		"karma-jasmine-html-reporter": "^2.1.0",
		"prettier": "^3.1.1",
		"typescript": "5.2.2",
		"typescript-strict-plugin": "^2.2.2-beta.1"
	},
	"developmentInfo": {
		"": "For Development you should use the local version of logigator-simulation to use live reload from the libary",
		"@logigator/logigator-simulation": "file:../../logigator-simulation"
	},
	"packageManager": "yarn@4.0.2"
}
